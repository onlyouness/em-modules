# Apache 2.2
<IfModule !mod_authz_core.c>
   <Files *.php>
       order allow,deny
       deny from all
   </Files>
   <Files Ets_crosssell_db.php>
          allow from all
      </Files>
</IfModule>

# Apache 2.4
<IfModule mod_authz_core.c>
   <FilesMatch ".+\.php$">
           Require all denied
       </FilesMatch>
   <Files Ets_crosssell_db.php>
       Require all granted
   </Files>
</IfModule>


